CREATE TABLE IF NOT EXISTS characters(
characterID INTEGER PRIMAARY KEY AUTIOINCREMENT,
characterName TEXT DEFAULT NULL,
stren INTEGER DEFAULT 0,
dex INTEGER;

CREATE TABLE IF NOT EXISTS characters(
characterID   INTEGER PRIMARY KEY AUTOINCREMENT,
characterName TEXT,
stren         INTEGER,
dex           INTEGER,
con           INTEGER,
intel         INTEGER,
wisdom        INTEGER,
charisma      INTEGER
);
